install.packages("haven")
library(haven)
library(data.table)
setDT(X3_Attitude)
summary(X3_Attitude)
X3_Attitude[, Condition := as.factor(Condition)]
X3_Attitude[, Attitude_hi := Attitude>median(Attitude)]
X3_Attitude[, Attitude_hi := as.factor(Attitude_hi)]
temp <- X3_Attitude[, .(FocalTrust_mean = mean(FocalTrust)), by=c("Condition","Attitude_hi")]
library(ggplot2)
ggplot(temp, aes(x=Attitude_hi, y=FocalTrust_mean, fill=as.factor(Condition))) + geom_col(position="dodge")
